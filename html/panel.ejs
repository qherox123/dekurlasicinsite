<%- include('ek/header', {bot, user, path}) %>
<script type="text/javascript">

</script>
<div class="container">
  <style>
    h8 {
  color: white;
  font-size: 25px;
  align: center;
  text-shadow: 2px 2px black;
  line-height: normal;
}
  h8{
    font-family:Montserrat,Open Sans,-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen,Ubuntu,Cantarell,Helvetica Neue,sans-serif
  }
    h8[id]{pointer-events:none}h1[id]:before{content:"";height:80px;margin:-80px 0 0}h1[id]:hover:after{opacity:1}h1[id]:after{-webkit-transition:opacity .1s ease-out;transition:opacity .1s ease-out;content:" \1F517";cursor:pointer;opacity:0;font-weight:100;pointer-events:auto
       }
        .panelss {
      background-color: #000;
      width: 350px;
      padding: 30px;
      padding: 25px;
      border-radius: 10px;
      margin-bottom: 8px;
      margin-right: 10px;
    }
    </style>
  
  <span align="center" style="color: white">
  <br><h1>All Bots</h1>
  <h5>It lists all the bots that belong to you!</h5>
    </span><br><br><style type="text/css">
.alonebutton {
	background-color:#525453;
	-webkit-border-top-left-radius:28px;
	-moz-border-radius-topleft:28px;
	border-top-left-radius:28px;
	-webkit-border-top-right-radius:28px;
	-moz-border-radius-topright:28px;
	border-top-right-radius:28px;
	-webkit-border-bottom-right-radius:28px;
	-moz-border-radius-bottomright:28px;
	border-bottom-right-radius:28px;
	-webkit-border-bottom-left-radius:28px;
	-moz-border-radius-bottomleft:28px;
	border-bottom-left-radius:28px;
text-indent:0;
	border:1px solid #303830;
	display:inline-block;
	color:#ffffff;
	font-family:arial;
	font-size:15px;
	font-weight:bold;
	font-style:normal;
height:50px;
	line-height:50px;
width:100px;
	text-decoration:none;
	text-align:center;
	text-shadow:1px 1px 0px #2f6627;
}.alonebutton:hover {
	background-color:#363636;
}.alonebutton:active {
	position:relative;
	top:1px;
}
       .kutucuk {
      background-color: #7289DA;
      width: 350px;
      padding: 30px;
      padding: 25px;
      border-radius: 10px;
      margin-bottom: 8px;
      margin-right: 10px;
      display: inline-block;

    }
  </style>
  <center>
  <% if (bot.ayar.has('botlar')) { %>
       <% if (bot.ayar.has(`kbotlar.${user.id}`)) { %>   

<%for(var i = 0; i < Object.keys(bot.ayar.fetch('botlar')).length; i++) {%>
<% var idd = Object.keys(bot.ayar.fetch('botlar'))[i] %>
 <%  if (bot.ayar.fetch(`botlar.${idd}.sahipid`) === user.id) { %>
        <div class="kutucuk">
       <center>
       <a href="/bot/<%=idd%>"><img src="<%=bot.ayar.fetch(`botlar.${idd}.avatar`)%>" class="card-img-top" alt="Bot Avatar" style="border-radius: 50%;border-shadow: 2px 2px" title="<%=bot.useful.seg(bot.ayar.fetch(`botlar.${idd}.isim`), 5)%>"></a>
      </center>
          <center>


      <%  if(bot.ayar.fetch(`botlar.${idd}.durum`) === 'Beklemede' || !bot.ayar.has(`botlar.${idd}.durum`)) { %>
       <span class="cards-beklemede">Waiting</span>
       <% } %>

       <%  if(bot.ayar.fetch(`botlar.${idd}.durum`) === 'Reddedilmiş') { %>
       <span class="card-reddedilmis">Denied</span>
       <% } %>

       <%  if(bot.ayar.fetch(`botlar.${idd}.durum`) === 'Onaylı') { %>
       <span class="cards-onayli">Apporved</span>
       <% } %>

       <span class="cards-oy"><strong style='color: white;'>Upvote: </strong><%=bot.ayar.fetch(`botlar.${idd}.oy`)||0%></span>
      <span class="cards-kutuphane"><%=bot.ayar.fetch(`botlar.${idd}.kutuphane`)%></span>
    </center>
                             <center>
                            <div class="cards-botisim"><%=bot.useful.seg(bot.ayar.fetch(`botlar.${idd}.isim`), 5)%></div>
                            <div class="cards-sahipisim"><%=bot.ayar.fetch(`botlar.${idd}.sahip`)%></div>     
                            <div class="cards-kisaaciklama"><%=bot.ayar.fetch(`botlar.${idd}.kisaaciklama`)%></div>
                          <br><br>
                        <div class="alonebutton">
                            <a href="/bot/<%=idd%>">View</a>
                            </div>
                         <div class="alonebutton">
                            
          <a href="/kullanici/<%=user.id%>/panel/<%=idd%>/duzenle">Edit</a>
                             </div>
          </center>
                       </div><br>
<% } %>
  <% } %>
</div>
      
      <% } else if (!bot.ayar.has(`kbotlar.${user.id}`)) {%>
           <h5 style="color:#222">
             Sistemde hiç sana ait bot bulunmuyor.
      </h5>
      <% } %>
      <% } else if (!bot.ayar.has('botlar')) {%>
           <h5 style="color:#222">
             Sistemde hiç bot bulunmuyor.
      </h5>
      <% } %>
      

 


</div>
</div></div>
<% include ./ek/footer %>
